<% if @placed_banners_second.present? %>
  <div class="banner12_contest_wrapper <%= params['controller'].to_s.split('/').try(&:first) %>">
    <ul class='banner12 <%= params['controller'].to_s.split('/').try(&:first) %>'>

      <% @placed_banners_second.each do |banner| %>
        <li class='js-banner'>
          <noindex>
          <%= link_to (image_tag banner.image_url, size: "880x80"), banner.url, :target => banner.target, rel: 'nofollow' %>
          </noindex>
        </li>
      <% end %>

    </ul>
  </div>
<% end %>
